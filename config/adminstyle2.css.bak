#frameutama {
	border-width: 2px;
	border-style: solid;
	border-color: #265180;
	border-collapse: collapse;
	bgcolor: #265180;
}

body {
	/*background-image: url(../images/background.gif);
	background-image: none;
	background-repeat: repeat-y;
	background-position: center;*/
	margin-left: 0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
}

#content {
	margin-left: 20px;
	margin-right: 8px;
font-family: Tahoma; 
	font-size: 10pt;
	/*padding: 20px 10px 0 0;*/
}
#content p {
	font-family: Tahoma; 
	font-size: 8pt;
	line-height: 1.8em;
	padding-left: 2em;
}


#content h2 {
	/*font: normal 120% Georgia;*/
	 font-family: Tahoma;
 	font-size:	16px;
	 font-weight: bold;

	color: #265180;
	background-color: transparent;
	border-bottom: 1px dotted #265180;
}
#content table {
	font-family: Tahoma; 
	font-size: 8pt;
	border-width: 1px;
	border-style: solid;
	border-color: #999999;
	border-collapse: collapse;
	margin: 10px 0px;
}
#content th{
	color: #FFFFFF;
	font-size: 7pt;
	text-transform: uppercase;
	text-align: center;
	padding: 0.5em;
	border-width: 1px;
	border-style: solid;
	border-color: #969BA5;
	border-collapse: collapse;
	background-color: #265180;
}
#content td{
	padding: 0.5em;
	vertical-align: top;
	border-width: 1px;
	border-style: solid;
	border-color: #969BA5;
	border-collapse: collapse;
}

#content input,textarea,select{
	font-family: Tahoma; 
	font-size: 8pt;
	vertical-align: top;
}


#content a:link,a:visited {
  color:#265180;
}
#content a:hover {
	color: #FF6600;
	text-decoration:none;
}

#tgl {
	margin-left: 0px;
	margin-right: 0px;
font-family: Tahoma; 
	font-size: 10pt;
	/*padding: 20px 10px 0 0;*/
}

#tgl table {
	font-family: Tahoma; 
	font-size: 8pt;
	border-width: 0px;
	border-style: none;
	margin: 0px 0px;
	
}

#tgl td{
	padding: 0;
	vertical-align: top;
	border-width: 0px;
	border-style: NONE;

letter-spacing:normal;
line-height:normal;
font-family: Tahoma;
font-size: 11px;
}

#tgl content input,textarea,select{
letter-spacing:.06em;
font-family:Verdana,Sans-Serif;
font-size:11px;
}


#paging{
	font-family: Tahoma; 
	font-size: 8pt; 
}

#footer{
	padding:  20px 0 20px 0px;
	 font-family: Verdana, Helvetica, Arial, sans-serif;
 	font-size:	9px;
	 font-weight: bold;

	color: #FFFFFF;
	background-color: #265180;
}

#navsite {
 font-family: Verdana, Helvetica, Arial, sans-serif;
 font-size: 0.6EM;
 width: 180px; 
 border-right: 1px solid #666; 
 padding: 0; 
 margin-bottom: 1em; 
 background-color: #265180; 
 color: #333; 
 
 }
#navsite p {
 display: none;}

#navsite ul {
 list-style: none; 
 margin: 0; 
 padding: 0;}
 

#navsite ul li {
 margin: 0; 
 border-top: 1px solid #003;
 color:#FFFFFF;
font-weight: bold;
 }


#navsite ul li a {
 display: block; 
 /*padding: 2px 2px 2px 0.5em; */
 
 /*padding: 10px 2px 10px 0.5em;*/
 padding: 5px 2px 5px 0.5em;

 border-left: 10px solid #369; 
 border-right: 10px solid #369;
 border-bottom: 1px solid #369;
 background-color: #56A7D8; 
 color: #fff; 
 text-decoration: none; 
 width: 100%;
 font-weight: normal;
 }

html>body #navsite ul li a { 
 width: auto; }

#navsite ul li a:hover {
	border-left: 10px solid #036;
	border-right: 10px solid #036;
	border-bottom: 1px solid #369;
	background-color: #265180;
	color: #003366;
	font-weight: bold;
}

img.thumbborder 
{
	  border-top-width: medium;
      border-bottom-width: medium;
      border-top-style: none;
      border-bottom-style: none;
      vertical-align:middle;
}